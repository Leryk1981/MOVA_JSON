{
  "name": "leryk-schemas-mova",
  "version": "3.4.1",
  "description": "MOVA Envelope schemas (JSON Schema, AJV 2020-12 compatible)",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist",
    "schemas"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./envelope": {
      "import": "./dist/envelope.js",
      "types": "./dist/envelope.d.ts"
    },
    "./schemas/*": "./schemas/*"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "test": "echo 'No tests yet'",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src"
  },
  "author": {
    "name": "Sergii Miasoiedov",
    "email": "contact@mova.io"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "file:../types-node",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  }
}
