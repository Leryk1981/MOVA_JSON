{
  "name": "leryk-sdk-mova",
  "version": "0.1.0",
  "description": "MOVA SDK: AJV validator, diagnostics mapper, completion suggestions, utilities",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "echo 'SDK tests'",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src"
  },
  "author": {
    "name": "Sergii Miasoiedov",
    "email": "contact@mova.io"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^2.1.1",
    "jsonc-parser": "^3.2.1",
    "leryk-schemas-mova": "3.4.1"
  },
  "devDependencies": {
    "@types/node": "file:../types-node",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "mocha": "^10.2.0",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  }
}
